<% script.create('JQuery')
  .add(`
$.term = {
    terms: {},
    getLog: function(name) {
        const self = this;
        $.get('/log/' + name)
            .done(function(json) {
                if (json.logs) {
                    const log = $(self.terms[name].log);
                    log.text(json.logs);
                    self.terms[name].time = json.time;
                }
            });
    },
    addLog: function(name, data) {
        const self = this;
        if (data.time > self.terms[name].time) {
            const log = $(self.terms[name].log);
            log.append(data.message + '\\r\\n');
            log.scrollTop(log[0].scrollHeight - log.height());
            self.terms[name].time = data.time;
        }
    },
    init: function(ports) {
        const self = this;
        for (let i = 0; i < ports.length; i++) {
            const port = ports[i];
            self.terms[port] = {
                log: $('.log.' + port)
            }
            self.getLog(port);
        }
    }
}
`); %>